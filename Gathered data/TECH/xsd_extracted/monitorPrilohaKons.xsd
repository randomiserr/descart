                        <xs:sequence>
                            <xs:element name="KodVykazuPKP" type="mon:KodVykazuPKPType"/>
                            <xs:element name="Polozka" type="mon:PolozkaType"/>
                            <xs:element name="ObdobiBezneCinnostHlavni"
                                type="mon:FinancniCastkaType"/>
                            <xs:element name="ObdobiMinuleCinnostHlavni"
                                type="mon:FinancniCastkaType"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
